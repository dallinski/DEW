<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dallinc.dewguides"
    android:versionCode="4"
    android:versionName="2.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dallinc.dewguides.MainPages"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dallinc.dewguides.Tango"
            android:label="@string/tango" >
        </activity>
        <activity
            android:name="com.dallinc.dewguides.InfoMenus"
            android:label="@string/title_activity_info_menus" >
        </activity>
        <activity
            android:name="com.dallinc.dewguides.GrandCircle"
            android:label="@string/grand_circle" >
        </activity>
        <activity
            android:name="com.dallinc.dewguides.BryceCanyon"
            android:label="@string/bryce_canyon" >
        </activity>
        <activity
            android:name="com.dallinc.dewguides.Sedona"
            android:label="@string/sedona" >
        </activity>
        <activity
            android:name="com.dallinc.dewguides.DriverLog"
            android:label="@string/driver_log"
            android:screenOrientation="sensorLandscape" >
        </activity>
    </application>

</manifest>